{% extends "base.twig" %}

{% block title %}Your Blog{% endblock %}

{% block leftcolumn %}
    {% for post in posts %}
    <div class="outh2">
        <div class="date">25 May</div>
        <div class="h2holder"><h2>{{ post.title }}</h2></div>
    </div>
    <div class="article">
        <div class="articlein">
            <p>{{ post.content }} <a href="#">Read More...</a></p>
        </div>
    </div>
    {% endfor %}
{% endblock %}